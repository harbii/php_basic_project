{
    "name"        : "harby/php_basic_project" ,
    "type"        : "project"                 ,
    "description" : "php project."            ,
    "license"     : "MIT"                     ,
    "keywords"    : [ "php" ]                 ,
    "autoload": {
        "psr-4": {
            "App\\": "."
        }
    },
    "require": {
        "vlucas/phpdotenv": "^5.5"
    },
    "require-dev": {
        "symfony/var-dumper": "^6.2"
    },
    "scripts": {
        "server" : "php -S localhost:8111" 
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
